{
    "compileOnSave": false,
    "compilerOptions": {
        "declaration": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "noImplicitThis": true,
        "outDir": "dist",
        "pretty": true,
        "removeComments": true,
        "sourceMap": false,
        "strict": true,
        "strictNullChecks": true,
        "target": "es5",
        "types": ["node"],
        "lib": ["es2015"]
    },
    "include": ["gen"]
}
